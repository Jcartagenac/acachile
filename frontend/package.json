{
  "name": "@acachile/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:prod": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy dist --project-name=acachile-prod",
    "deploy:auto": "node scripts/intelligent-deploy.cjs",
    "setup-bindings": "node scripts/dashboard-helper.cjs",
    "check-bindings": "curl -s https://acachile-prod.pages.dev/api/bindings | jq .",
    "health-check": "curl -s https://acachile-prod.pages.dev/api/health | jq ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@types/js-cookie": "^3.0.6",
    "clsx": "^2.1.1",
    "esbuild": "^0.25.10",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
