{
  "name": "@acachile/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "build:prod": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 src/utils/cn.ts src/utils/cn.test.ts ../shared/utils/validators.ts functions/api/system/__tests__/health.utils.test.ts",
    "typecheck": "npm run typecheck:frontend && npm run typecheck:backend",
    "typecheck:frontend": "tsc --noEmit -p tsconfig.typecheck.json",
    "typecheck:backend": "tsc --noEmit -p functions/tsconfig.typecheck.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=acachile",
    "deploy:auto": "node scripts/intelligent-deploy.cjs",
    "setup-bindings": "node scripts/dashboard-helper.cjs",
    "check-bindings": "curl -s https://acachile.pages.dev/api/bindings | jq .",
    "health-check": "curl -s https://acachile.pages.dev/api/health | jq ."
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.25.1",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@hookform/resolvers": "^3.9.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "next",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.3.5",
    "vitest": "^4.0.4"
  }
}
