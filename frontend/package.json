{
  "name": "@acachile/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:prod": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=acachile-prod",
    "deploy:auto": "node scripts/intelligent-deploy.cjs",
    "setup-bindings": "node scripts/dashboard-helper.cjs",
    "check-bindings": "curl -s https://acachile-prod.pages.dev/api/bindings | jq .",
    "health-check": "curl -s https://acachile-prod.pages.dev/api/health | jq ."
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.545.0",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.25.1",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@hookform/resolvers": "^3.9.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.7.2",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "next",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.3.5"
  }
}
